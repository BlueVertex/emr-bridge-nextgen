{
    "name": "bluevertex/emr-bridge-nextgen",
    "description": "An EMR Bridge Implementation for NextGen Beta API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Fox",
            "email": "matt.fox@thebluevertex.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../emr-bridge",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/contracts": "~5.5",
        "illuminate/support": "~5.5",
        "bluevertex/emr-bridge": "dev-master",
        "kitetail/zttp": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "BlueVertex\\EMRBridgeNextGen\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BlueVertex\\EMRBridgeNextGen\\EMRBridgeNextGenServiceProvider"
            ],
            "aliases": {}
        }
    }
}
